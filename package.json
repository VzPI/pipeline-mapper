{
  "name": "mmsc_vlp",
  "version": "1.0.0",
  "description": "Map/navigation tool",
  "main": "index.js",
  "scripts": {
    "bundle": "./node_modules/browserify/bin/cmd.js build/main.js -o public/scripts/bundle.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "npm run lint && npm run bundle && concurrently \"npm run ssl-proxy\" \"nodemon\"",
    "start:prod": "node index.js",
    "ssl-proxy": "local-ssl-proxy --source 3001 --target 3000",
    "lint": "eslint ./build/*.js ./*.js || exit 0",
    "postinstall": "npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/mmsctedc/mmsc_vlp.git"
  },
  "author": "Darren Klein",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/mmsctedc/mmsc_vlp/issues"
  },
  "homepage": "https://gitlab.com/mmsctedc/mmsc_vlp#README",
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "concurrently": "^3.5.0",
    "eslint": "^4.7.2",
    "local-ssl-proxy": "^1.3.0",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "browserify": "^14.4.0",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "heroku-ssl-redirect": "0.0.4",
    "if-env": "^1.0.0",
    "leaflet": "^1.2.0",
    "leaflet-pulse-icon": "^0.1.0"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "4.0.5"
  }
}
